                                            <span t-field="o.gross_weight"/>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <!-- endregion -->
                            <!-- region DESTINO DE TRASLADO -->
                            <table style="width: 100%; border:1px solid black; border-collapse: collapse;margin-top: 10px;">
                                <thead>
                                    <tr style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                        <th colspan="6">
                                            <strong>TRANSFER DESTINATION</strong>
                                        </th>

