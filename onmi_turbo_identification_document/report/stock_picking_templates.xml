                            <table style="width: 100%; border:1px solid black; border-collapse: collapse;margin-top: 10px;">
                                <thead>
                                    <tr style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                        <th colspan="6">
                                            <strong>CARRIER</strong>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td colspan="1"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <strong>First Transfer: Init date:</strong>
                                        </td>
                                        <td colspan="1"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">

                                        </td>
                                        <td colspan="1"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <strong>Delivery date:</strong>
                                        </td>
                                        <td colspan="1"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <span t-field="o.date_done" t-options="{'widget': 'date'}"/>
                                        </td>
                                        <td colspan="1"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <strong># License Plate</strong>
                                        </td>
                                        <td colspan="1"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <span t-field="o.license_plate_id.name"/>
                                        </td>
                                    </tr>
                                    <t t-call="onmi_turbo_identification_document.onmi_tbody_company_for_identification_document"/>
                                    <tr>
                                        <td style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <strong>User:</strong>
                                        </td>
                                        <td colspan="5"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">

                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <!-- endregion -->
                            <!-- region FIRMAS -->
                            <table style="width: 100%; border:1px solid black; border-collapse: collapse;margin-top: 10px;">
                                <tr height="70px">
                                    <td width="34%" style="border:1px solid black; border-collapse: collapse;">
                                        <p>Accepted:
                                            <i class="fa fa-square-o">Yes</i>
                                            <i class="fa fa-square-o">No</i>
                                            <br/>
                                            Incidences:
                                        </p>

                                        <br/>
                                        Date:
                                    </td>
                                    <td width="33%"
                                        style="border:1px solid black; border-collapse: collapse;vertical-align: text-top;">
                                        ORIGIN sign:
                                        <t t-if="o.picking_type_code == 'outgoing'">
                                            <br/>
                                            <!-- Cambiado de to_text a image_data_uri -->
                                            <img t-if="o.company_id.stamp"
                                                 t-att-src="image_data_uri(o.company_id.stamp)"
                                                 style="margin-left:50px;max-width: 150px; opacity: 100%;"/>
                                        </t>
                                    </td>
                                    <td width="33%"
                                        style="border:1px solid black; border-collapse: collapse;vertical-align: text-top;">
                                        DESTINY sign:
                                        <t t-if="o.picking_type_code == 'incoming'">
                                            <br/>
                                            <!-- Cambiado de to_text a image_data_uri -->
                                            <img t-if="o.company_id.stamp"
                                                 t-att-src="image_data_uri(o.company_id.stamp)"
                                                 style="margin-left:50px;max-width: 150px; opacity: 50%;"/>
                                        </t>
                                    </td>
                                </tr>
                                <tr>
                                    <td width="34%" style="border:1px solid black; border-collapse: collapse;">
                                        Quantity received:
                                    </td>
                                    <td width="33%" style="border:1px solid black; border-collapse: collapse;">
                                        D/Dª. (Name and Surname)
                                        <t t-if="o.picking_type_code == 'outgoing'">
                                            <span t-field="o.company_id.name"/>
                                        </t>
                                    </td>
                                    <td width="33%" style="border:1px solid black; border-collapse: collapse;">
                                        D/Dª.

                                        <t t-if="o.picking_type_code == 'incoming'">
                                            <span t-field="o.company_id.name"/>
                                        </t>
                                    </td>
                                </tr>
                            </table>
                            <!-- endregion -->
                            <p style="page-break-after: always;"/>
                        </div>
                    </t>
                </t>
            </t>
        </template>
        <template id="onmi_report_identification_document">
            <t t-foreach="docs" t-as="o">
                <t t-call="onmi_turbo_identification_document.onmi_report_identification_document_document"
                   t-lang="o.partner_id.lang"/>
            </t>
        </template>
