        <!-- Template unit Identification Document-->
        <template id="onmi_report_identification_document_document">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="onmi_turbo_identification_document.onmi_external_layout_stock_pickings_identification_document">
                        <div class="page">
                            <style>
                                th { background-color:#D3D3D3; }
                                tr,td { font-size: 12px !important; overflow: hidden;}
                                strong, span {margin-left: 5px !important; margin-right: 5px !important;}

                            </style>
                            <!-- region OPERADOR DE TRASLADO-->
                            <table style="width: 100%; border:1px solid black; border-collapse: collapse;margin-top: 60px;">
                                <thead>
                                    <tr style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                        <th colspan="6">
                                            <strong>TRANSFER OPERATOR</strong>
                                        </th>

                                    </tr>
                                </thead>
                                <tbody>
                                    <t t-call="onmi_turbo_identification_document.onmi_tbody_company_for_identification_document"/>
                                    <tr>
                                        <td style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <strong>User:</strong>
                                        </td>
                                        <td colspan="5"
                                            style="border:1px solid black; border-collapse: collapse; text-align: left;">
                                            <span t-field="o.company_id.partner_id.document_user_id.name"/>
                                        </td>
                                    </tr>
                                    <tr>
