<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="onmi_report_invoice_exportacion_inherit_accounts"
                  inherit_id="onmi_turbo_credit_note.onmi_report_exportacion_content">
            <xpath expr="//div[@name='accounts']" position="inside">
                <t t-if="o.selected_export_bank_id">
                    <span t-field="o.selected_export_bank_id.display_text"
                          style="white-space: pre-line;"/>
                </t>
            </xpath>
        </template>
    </data>
</odoo>
