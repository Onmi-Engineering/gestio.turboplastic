<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="onmi_view_account_move_booking_inherit12" model="ir.ui.view">
            <field name="name">onmi.view.account.move.iban.inherit12</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='edi_state']" position="after">
                    <!-- <field name="x_studio_exportacion" invisible="1"/> -->
                    <field name="booking_track" />
                </xpath>
            </field>
        </record>

        <record id="onmi_view_account_move_tree_booking_inherit12" model="ir.ui.view">
            <field name="name">onmi.view.account.move.iban.inherit12</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_in_invoice_bill_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='status_in_payment']" position="after">
                    
                    <field name="booking_track" optional="hide"/>
                </xpath>
            </field>
        </record>
        <record id="onmi_view_account_move_tree_booking_inherit12out" model="ir.ui.view">
            <field name="name">onmi.view.account.move.iban.inherit12out</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_out_invoice_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='status_in_payment']" position="after">
                    
                    <field name="booking_track" optional="hide"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo> 
<!-- attrs="{'invisible':[('x_studio_exportacion','=',False)]}" -->