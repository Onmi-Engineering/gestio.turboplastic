<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="onmi_view_move_line_tree_inherit_partner_picking" model="ir.ui.view">
            <field name="name">stock.move.line.tree.inherit.partner.picking</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_move_line_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='reference']" position="after">
                    <field name="partner_picking_id" string="Partner of Picking"/>
                </xpath>
            </field>
        </record>

        <record id="onmi_view_move_line_form_inherit_partner_picking" model="ir.ui.view">
            <field name="name">stock.move.line.form.inherit.partner.picking</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_move_line_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='reference']" position="after">
                    <field name="partner_picking_id" string="Partner of Picking"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>